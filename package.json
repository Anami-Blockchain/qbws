{
  "name": "qbws",
  "version": "0.0.1",
  "author": "John Ballantyne <johnballantyne+npmjs@gmail.com>",
  "description": "a web server for QuickBooks Web Connector integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnballantyne/qbws"
  },
  "keywords": [
    "qbwc",
    "QuickBooks",
    "QuickBooks Web Connector"
  ],
  "dependencies" : {
    "colors"   :  "*",
    "soap"     :  "0.4.x"
  },
  "scripts": {
    "jshint": "jshint index.js test/client.js",
    "test": "npm run-script jshint"
  },
  "devDependencies": {
    "jshint": "2.1.x"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "es3": false,
    "es5": false,
    "forin": true,
    "freeze": true,
    "funcscope": false,
    "futurehostile": true,
    "latedef": "nofunc",
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": false,
    "shadow": "outer",
    "singleGroups": true,
    "undef": true,
    "unused": true,
    "varstmt": false,
    "asi": false,
    "boss": false,
    "esnext": false,
    "evil": false,
    "expr": false,
    "loopfunc": false,
    "plusplus": true,
    "node": true
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.8"
  }
}
